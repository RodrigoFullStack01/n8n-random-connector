{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "Um nó personalizado para o n8n que gera números aleatórios com a API do Random.org.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Rodrigo Duarte",
    "email": "leandro.rodrigo05@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RodrigoFullStack01/n8n-random-connector.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write credentials/**/*.ts nodes/**/*.ts",
    "lint": "eslint credentials/**/*.ts nodes/**/*.ts",
    "lint:fix": "eslint credentials/**/*.ts nodes/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js credentials/**/*.ts nodes/**/*.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/core": "*",
    "@n8n/workflow": "*",
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  }
}